#!/usr/bin/env python

from sys import stdin

for line in stdin:
    if line.startswith("#"): continue
    tokens = line.strip().split('\t')

    sample, chromosome, loci, alleles, vaf = tokens[0].split(":")
    start, end = loci.split("-")
    reference, alternate = alleles.split("/")

    if tokens[2] != alternate: continue
    consequence = tokens[6]
    
    extra = tokens[-1].split(";")
    gene = "-"
    ensgene = tokens[3]
    transcript = tokens[4]
    canonical = "NO"

    protein_position = tokens[9]
    amino_acids = tokens[10]
    existing_variation = tokens[12]


    for ext in extra:
        if ext.startswith("SYMBOL="):
            gene = ext.split("=")[1]  
        if ext.startswith("CANONICAL="):
            canonical = ext.split("=")[1]

    output = [sample, chromosome, start, end, reference, alternate, consequence, gene,
    ensgene, transcript, protein_position, amino_acids, existing_variation,
    canonical, vaf]

    print "\t".join(map(str, output))
